    { "id": "v-0205", "ro": "La revedere", "en": "Goodbye (formal)", "uk": "До побачення", "pronunciation": "lah reh-veh-DEH-reh", "partOfSpeech": "phrase", "example": { "ro": "La revedere și mulțumesc!", "en": "Goodbye and thank you!", "uk": "До побачення і дякую!" }, "tags": ["greeting", "formal", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0206", "ro": "Pa / Pa-pa", "en": "Bye (informal)", "uk": "Бувай / Па-па", "pronunciation": "pah / pah-pah", "partOfSpeech": "phrase", "example": { "ro": "Pa! Ne vedem mâine.", "en": "Bye! See you tomorrow.", "uk": "Бувай! Побачимось завтра." }, "tags": ["greeting", "informal"], "difficulty": 1 },
    { "id": "v-0207", "ro": "Mă numesc...", "en": "My name is...", "uk": "Мене звати...", "pronunciation": "muh noo-MESK", "partOfSpeech": "phrase", "example": { "ro": "Mă numesc Andrei Popescu.", "en": "My name is Andrei Popescu.", "uk": "Мене звати Андрій Попеску." }, "tags": ["introduction", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0208", "ro": "Cum vă numiți?", "en": "What is your name? (formal)", "uk": "Як вас звуть? (формальне)", "pronunciation": "koom vuh noo-MEETS", "partOfSpeech": "phrase", "example": { "ro": "Cum vă numiți, vă rog?", "en": "What is your name, please?", "uk": "Як вас звуть, будь ласка?" }, "tags": ["question", "formal", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0209", "ro": "Sunt din Ucraina.", "en": "I am from Ukraine.", "uk": "Я з України.", "pronunciation": "soont deen oo-krah-EE-nah", "partOfSpeech": "phrase", "example": { "ro": "Sunt din Ucraina, din orașul Kyiv.", "en": "I am from Ukraine, from the city of Kyiv.", "uk": "Я з України, з міста Київ." }, "tags": ["introduction", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0210", "ro": "Sunt din...", "en": "I am from...", "uk": "Я з...", "pronunciation": "soont deen", "partOfSpeech": "phrase", "example": { "ro": "Sunt din orașul Odesa.", "en": "I am from the city of Odessa.", "uk": "Я з міста Одеса." }, "tags": ["introduction", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0211", "ro": "Locuiesc în...", "en": "I live in...", "uk": "Я живу в...", "pronunciation": "loh-koo-YESK uhn", "partOfSpeech": "phrase", "example": { "ro": "Locuiesc în București de trei ani.", "en": "I have been living in Bucharest for three years.", "uk": "Я живу в Бухаресті три роки." }, "tags": ["introduction", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0212", "ro": "Am ... ani.", "en": "I am ... years old.", "uk": "Мені ... років.", "pronunciation": "ahm ... ahn", "partOfSpeech": "phrase", "example": { "ro": "Am treizeci și cinci de ani.", "en": "I am thirty-five years old.", "uk": "Мені тридцять п'ять років." }, "tags": ["introduction", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0213", "ro": "Sunt căsătorit / căsătorită.", "en": "I am married (m/f).", "uk": "Я одружений / одружена.", "pronunciation": "soont kuh-suh-toh-REET / kuh-suh-toh-REE-tuh", "partOfSpeech": "phrase", "example": { "ro": "Sunt căsătorit și am doi copii.", "en": "I am married and have two children.", "uk": "Я одружений і маю двох дітей." }, "tags": ["introduction", "ceremony-essential"], "difficulty": 2 },
    { "id": "v-0214", "ro": "Lucrez ca...", "en": "I work as...", "uk": "Я працюю як...", "pronunciation": "LOO-krez kah", "partOfSpeech": "phrase", "example": { "ro": "Lucrez ca inginer.", "en": "I work as an engineer.", "uk": "Я працюю інженером." }, "tags": ["introduction", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0215", "ro": "Cum sunteți?", "en": "How are you? (formal)", "uk": "Як ви? (формальне)", "pronunciation": "koom soon-TETS", "partOfSpeech": "phrase", "example": { "ro": "Bună ziua! Cum sunteți?", "en": "Good day! How are you?", "uk": "Добрий день! Як ви?" }, "tags": ["greeting", "formal"], "difficulty": 1 },
    { "id": "v-0216", "ro": "Sunt bine, mulțumesc.", "en": "I am fine, thank you.", "uk": "Я добре, дякую.", "pronunciation": "soont BEE-neh mool-tsoo-MESK", "partOfSpeech": "phrase", "example": { "ro": "Sunt bine, mulțumesc. Dumneavoastră?", "en": "I'm fine, thank you. And you?", "uk": "Я добре, дякую. А ви?" }, "tags": ["greeting", "essential"], "difficulty": 1 },
    { "id": "v-0217", "ro": "Îmi pare bine.", "en": "Nice to meet you. / Pleased.", "uk": "Приємно познайомитись. / Радий.", "pronunciation": "uhm PAH-reh BEE-neh", "partOfSpeech": "phrase", "example": { "ro": "Îmi pare bine de cunoștință.", "en": "Nice to meet you.", "uk": "Приємно познайомитись." }, "tags": ["greeting", "formal"], "difficulty": 1 },
    { "id": "v-0218", "ro": "Scuzați-mă", "en": "Excuse me (formal)", "uk": "Вибачте (формальне)", "pronunciation": "skoo-ZAHTS-muh", "partOfSpeech": "phrase", "example": { "ro": "Scuzați-mă, nu am înțeles.", "en": "Excuse me, I didn't understand.", "uk": "Вибачте, я не зрозумів." }, "tags": ["politeness", "formal", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0219", "ro": "Nu am înțeles.", "en": "I didn't understand.", "uk": "Я не зрозумів/зрозуміла.", "pronunciation": "noo ahm uhn-TSEH-les", "partOfSpeech": "phrase", "example": { "ro": "Scuzați-mă, nu am înțeles. Puteți repeta?", "en": "Excuse me, I didn't understand. Can you repeat?", "uk": "Вибачте, я не зрозумів. Можете повторити?" }, "tags": ["essential", "ceremony-essential"], "difficulty": 2 },
    { "id": "v-0220", "ro": "Puteți repeta, vă rog?", "en": "Can you repeat, please? (formal)", "uk": "Можете повторити, будь ласка? (формальне)", "pronunciation": "poo-TETS reh-peh-TAH vuh rog", "partOfSpeech": "phrase", "example": { "ro": "Puteți repeta, vă rog? Mai rar.", "en": "Can you repeat, please? More slowly.", "uk": "Можете повторити, будь ласка? Повільніше." }, "tags": ["essential", "formal", "ceremony-essential"], "difficulty": 2 },
    { "id": "v-0221", "ro": "Puteți vorbi mai rar?", "en": "Can you speak more slowly? (formal)", "uk": "Можете говорити повільніше? (формальне)", "pronunciation": "poo-TETS vohr-BEE my rahr", "partOfSpeech": "phrase", "example": { "ro": "Vă rog, puteți vorbi mai rar?", "en": "Please, can you speak more slowly?", "uk": "Будь ласка, можете говорити повільніше?" }, "tags": ["essential", "formal", "ceremony-essential"], "difficulty": 2 },
    { "id": "v-0222", "ro": "domn", "en": "sir / mister", "uk": "пан", "pronunciation": "dohm-n", "partOfSpeech": "noun", "example": { "ro": "Bună ziua, domnule!", "en": "Good day, sir!", "uk": "Добрий день, пане!" }, "tags": ["formal", "title"], "difficulty": 1 },
    { "id": "v-0223", "ro": "doamnă", "en": "madam / mrs.", "uk": "пані", "pronunciation": "DWAHM-nuh", "partOfSpeech": "noun", "example": { "ro": "Bună ziua, doamnă!", "en": "Good day, madam!", "uk": "Добрий день, пані!" }, "tags": ["formal", "title", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0224", "ro": "domnișoară", "en": "miss", "uk": "панна", "pronunciation": "dohm-nee-SHWAH-ruh", "partOfSpeech": "noun", "example": { "ro": "Bună ziua, domnișoară!", "en": "Good day, miss!", "uk": "Добрий день, панно!" }, "tags": ["formal", "title"], "difficulty": 1 },
    { "id": "v-0225", "ro": "Cu plăcere.", "en": "You're welcome. / With pleasure.", "uk": "Будь ласка. / Із задоволенням.", "pronunciation": "koo pluh-CHEH-reh", "partOfSpeech": "phrase", "example": { "ro": "— Mulțumesc! — Cu plăcere!", "en": "— Thank you! — You're welcome!", "uk": "— Дякую! — Будь ласка!" }, "tags": ["politeness", "essential"], "difficulty": 1 },
    { "id": "v-0226", "ro": "Vorbesc românește.", "en": "I speak Romanian.", "uk": "Я говорю румунською.", "pronunciation": "vohr-BESK roh-muh-NESHT-eh", "partOfSpeech": "phrase", "example": { "ro": "Da, vorbesc românește.", "en": "Yes, I speak Romanian.", "uk": "Так, я говорю румунською." }, "tags": ["language", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0227", "ro": "Studiez limba română de...", "en": "I have been studying Romanian for...", "uk": "Я вивчаю румунську мову...", "pronunciation": "STOO-dee-ez LEEM-bah roh-MUH-nuh deh", "partOfSpeech": "phrase", "example": { "ro": "Studiez limba română de un an.", "en": "I have been studying Romanian for one year.", "uk": "Я вивчаю румунську мову один рік." }, "tags": ["language", "ceremony-essential"], "difficulty": 2 },
    { "id": "v-0228", "ro": "Doresc cetățenia română.", "en": "I want Romanian citizenship.", "uk": "Я хочу румунське громадянство.", "pronunciation": "doh-RESK cheh-tuh-TSEH-nee-ah roh-MUH-nuh", "partOfSpeech": "phrase", "example": { "ro": "Doresc cetățenia română deoarece...", "en": "I want Romanian citizenship because...", "uk": "Я хочу румунське громадянство тому що..." }, "tags": ["ceremony-essential"], "difficulty": 2 },
    { "id": "v-0229", "ro": "Sunt pregătit / pregătită.", "en": "I am ready (m/f).", "uk": "Я готовий / готова.", "pronunciation": "soont preh-guh-TEET / preh-guh-TEE-tuh", "partOfSpeech": "phrase", "example": { "ro": "Da, sunt pregătit pentru depunerea jurământului.", "en": "Yes, I am ready for taking the oath.", "uk": "Так, я готовий до складання присяги." }, "tags": ["ceremony-essential"], "difficulty": 2 },
