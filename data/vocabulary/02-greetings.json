{
  "moduleId": "mod-02",
  "words": [
    { "id": "v-0201", "ro": "Bună ziua", "en": "Good day / Hello (formal)", "uk": "Добрий день (формальне)", "pronunciation": "BOO-nuh ZEE-wah", "partOfSpeech": "phrase", "example": { "ro": "Bună ziua, mă numesc Maria.", "en": "Good day, my name is Maria.", "uk": "Добрий день, мене звати Марія." }, "notes": { "en": "Formal daytime greeting. Use this at the ceremony.", "uk": "Формальне привітання вдень. Використовуйте на церемонії." }, "tags": ["greeting", "formal", "ceremony-essential"], "difficulty": 1 },
    { "id": "v-0202", "ro": "Bună dimineața", "en": "Good morning", "uk": "Добрий ранок", "pronunciation": "BOO-nuh dee-mee-NYAH-tsah", "partOfSpeech": "phrase", "example": { "ro": "Bună dimineața! Cum vă simțiți?", "en": "Good morning! How do you feel?", "uk": "Добрий ранок! Як ви себе почуваєте?" }, "tags": ["greeting", "formal"], "difficulty": 1 },
